							},
						},
						"S"-E_TABLETYPE.BUSINESS-'B-business table':{
							auditSub = regAuditSubBusi(field, beforeValue, afterValue, auditSeq, (long)n);:{
								apl_business_audit_sub	'登记业务审计子表'
							}
						}
					}
					auditSubInfo.add(auditSub);
					n++;
				},
				DaoUtil.insertBatch(auditSub, auditSubInfo);	'批量插入'
			},
			regAudit(newRecord, E_DATAOPERATE.MODIFY, tableType, table, auditSeq);	'审计主表处理,没有字段变化，不插入主表'
		}	
